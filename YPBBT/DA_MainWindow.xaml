                <TextBlock x:Name="Processing_Status_TextBlock" TextWrapping="Wrap" TextAlignment="Center" Text="{}{Msg}" FontFamily="Yu Gothic" Foreground="#FFF3F3F3" Margin="0,1,0,0" Grid.Row="1" VerticalAlignment="Top"/>
